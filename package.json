{
  "name": "react-archux-example",
  "description": "React + Archux Example",
  "version": "1.0.3",
  "main": "server/server.js",
  "scripts": {
    "build-client": "NODE_ENV=production webpack --config client/webpack.config.js -p --progress",
    "build-client:watch": "cross-env NODE_ENV=development webpack --config client/webpack.config.js -p --progress --watch",
    "lint": "eslint --ext .jsx,.js ./client/app/src",
    "test": "mocha --require mock-css-modules --reporter spec client/.setup.js client/app/src/**/*.test.js --timeout 60000",
    "e2e": "webdriver-manager update && protractor",
    "security": "nsp check",
    "coverage": "nyc mocha --require mock-css-modules --reporter spec client/.setup.js client/app/src/**/*.test.js --timeout 60000",
    "pretest": "npm run lint",
    "jsdoc": "jsdoc client/app/src -r -d docs/client-docs -R README.md -P package.json"
  },
  "pre-commit": [
    "test"
  ],
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=3.10.8"
  },
  "author" : "artur.basak@instinctools.ru",
  "repository" : {
    "type": "git",
    "url": "https://github.com/archik408/react-archux-example.git"
  },
  "license" : "MIT",
  "homepage": "https://github.com/archik408/react-archux-example",
  "keywords": [
    "react",
    "unidirectional dataflow",
    "redux",
    "flux",
    "action-dispatcher",
    "dispatcher",
    "store",
    "observer",
    "react example",
    "custom flux",
    "app example",
    "client-side example",
    "archux"
  ],

